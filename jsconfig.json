{
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "components/*": ["src/components/*"],
      "pages/*": ["src/pages/*"],
      "theme": ["src/styles"],
      "theme/*": ["src/styles/*"],
      "assets/jpg/*": ["src/assets/jpg"],
      "assets/*": ["src/assets"],
      "views/*": ["src/views/*"],
      "routes/*": ["src/routes/*"]
    }
  },
  "include": ["src"]
}
